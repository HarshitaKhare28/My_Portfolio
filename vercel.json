{
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)\\.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/assets/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
